
config board 'board'
	option hardware 'EG200'
	option hasAdsl '0'
	option hasVdsl '0'
	option hasVoice '1'
	option hasDect '0'
	option VoicePorts '1'
	option VoiceLeds '1'
	option hasWifi '1'
	option ethernetPorts '5'
	option ethernetPortNames 'LAN1 LAN2 LAN3 GbE WAN'
	option ethernetPortOrder 'eth2 eth1 eth0 eth6 eth3'
	option ethernetLanPorts 'eth2 eth1 eth0 eth6'
	option ethernetWanPort 'eth3'
	option adslWanPort ''
	option vdslWanPort ''
	option lednames 'Power Eth WLAN1 WLAN2 WPS VoIP1 GeWAN INET'
	option buttonnames 'RESET WPS WIFI'
	option wpsButtonGpio '0'
	option hasBrcmAvs '1'
	option i2cGpioCustom 'bus0=0,10,9'

config wifi-chip '435f'
        option bands 'a'

config wifi-chip 'a8db'
        option bands 'b'

config buttons 'buttons'
	option RESET 'gpio 32 al resetbutton'
	option WPS 'gpio 33 al wpsbutton'
	option WIFI 'gpio 8 al ecobutton'

config leds 'leds'
	option Power_green 'gpio 21 ah'
	option Power_red 'gpio 20 al'
	option Eth_red 'sr 0 ah'
	option Eth_green 'sr 9 ah'
	option WLAN1_red 'sr 10 ah'
	option WLAN1_green 'sr 2 ah'
	option WPS_green 'sr 1 ah'

config led_map 'led_map'
	option dsl_ok 'DSL_red=ON'
	option dsl_off 'DSL_red=OFF'
	option dsl_notice 'DSL_red=BLINK_SLOW'
	option dsl_alert 'DSL_red=BLINK_SLOW'
	option dsl_error 'DSL_red=BLINK_FAST'
	option wifi_ok 'WLAN1_green=ON'
	option wifi_off 'WLAN1_green=OFF'
	option wifi_notice 'WLAN1_green=BLINK_SLOW'
	option wifi_alert 'WLAN1_green=BLINK_SLOW'
	option wifi_error 'WLAN1_green=BLINK_FAST'
	option wps_ok 'WPS_green=ON'
	option wps_off 'WPS_green=OFF'
	option wps_notice 'WPS_green=BLINK_SLOW'
	option wps_alert 'WPS_green=OFF'
	option wps_error 'WPS_green=OFF'
	option lan_ok 'Ethernet_green=ON'
	option lan_off 'Ethernet_green=OFF'
	option lan_notice 'Ethernet_green=BLINK_SLOW'
	option lan_alert 'Ethernet_green=BLINK_SLOW'
	option lan_error 'Ethernet_green=BLINK_FAST'
	option status_ok 'Status_green=ON Status_red=OFF'
	option status_off 'Status_green=OFF Status_red=OFF'
	option status_notice 'Status_green=BLINK_SLOW Status_red=OFF'
	option status_alert 'Status_green=OFF Status_red=BLINK_SLOW'
	option status_error 'Status_green=OFF Status_red=BLINK_FAST'
	option dect_ok ''
	option dect_off ''
	option dect_notice ''
	option dect_alert ''
	option dect_error ''
	option tv_ok ''
	option tv_off ''
	option tv_notice ''
	option tv_alert ''
	option tv_error ''
	option usb_ok 'USB_green=ON'
	option usb_off 'USB_green=OFF'
	option usb_notice 'USB_green=BLINK_SLOW'
	option usb_alert 'USB_green=BLINK_SLOW'
	option usb_error 'USB_green=BLINK_FAST'
	option wan_ok ''
	option wan_off 'WAN_green=OFF WAN_yellow=OFF'
	option wan_notice 'WAN_green=ON'
	option wan_alert 'WAN_yellow=ON'
	option wan_error ''
	option internet_ok 'Internet_green=ON Internet_red=OFF'
	option internet_off 'Internet_green=OFF Internet_red=OFF'
	option internet_notice 'Internet_green=BLINK_SLOW Internet_red=OFF'
	option internet_alert 'Internet_green=OFF Internet_red=BLINK_SLOW'
	option internet_error 'Internet_green=OFF Internet_red=ON'
	option voice1_ok ''
	option voice1_off ''
	option voice1_notice ''
	option voice1_alert ''
	option voice1_error ''
	option voice2_ok ''
	option voice2_off ''
	option voice2_notice ''
	option voice2_alert ''
	option voice2_error ''
	option eco_ok ''
	option eco_off ''
	option eco_notice ''
	option eco_alert ''
	option eco_error ''
	option gbe_ok ''
	option gbe_off 'GbE_green=OFF GbE_yellow=OFF'
