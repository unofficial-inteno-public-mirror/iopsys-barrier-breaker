
config board 'board'
	option hardware 'CG301'
	option hasAdsl '0'
	option hasVdsl '0'
	option hasVoice '0'
	option hasDect '1'
	option VoicePorts '0'
	option VoiceLeds '0'
	option hasWifi '1'
	option ethernetPorts '2'
	option ethernetPortNames 'LAN WAN'
	option ethernetPortOrder 'eth1 eth0'
	option ethernetLanPorts 'eth1'
	option ethernetWanPort 'eth0'
	option adslWanPort ''
	option vdslWanPort ''
	option lednames 'Status Communication Service Pair Cancel WAN'
	option buttonnames 'RESET Communication Service Pair Cancel TOUCH_NEAR TOUCH_FAR'
	option wpsButtonGpio '0'
	option fxsRelayGpio '0'
	option i2cGpioCustom 'bus0=0,23,22'

config wifi-chip '435f'
	option bands 'b'

config wifi-chip '43a0'
	option bands 'a'
	option is_ac '1'

config wifi-chip '43a2'
	option bands 'a'
	option is_ac '1'

config wifi-chip '43bc'
        option bands 'a'
        option is_ac '1'

config periph_ctrl 'periph_ctrl'
	option bt_en           'gpio 14 ah'
	option bt_power_ctrl   'gpio 19 ah'
	option usb1_power_ctrl 'gpio 36 ah'
	option usb2_power_ctrl 'gpio 37 ah'

config buttons 'buttons'
	option RESET         'gpio 32 al resetbutton Status_white'
	option Communication 'i2c 5 ah communicationbutton Communication_white'
	option Service       'i2c 1 ah servicebutton Service_white'
	option Pair          'i2c 3 ah pairbutton Pair_white'
	option Cancel        'i2c 4 ah cancelbutton Cancel_red'
	option TOUCH_NEAR    'i2c 8 ah touch_near Service_red'
	option TOUCH_FAR     'i2c 9 ah touch_far Communication_red'

config leds 'leds'
	option Status_green        'gpio 39 al'
	option Status_red          'gpio 38 al'
	option Status_white        'i2c 0 ah'
	option Communication_white 'i2c 5 ah'
	option Communication_red   'i2c 6 ah'
	option Service_white       'i2c 1 ah'
	option Service_red         'i2c 2 ah'
	option Pair_white          'i2c 3 ah'
	option Cancel_red          'i2c 4 ah'
	option WAN_green           'gpio 9 al'
	option WAN_yellow          'gpio 10 al'

config led_map 'led_map'
	option dsl_ok ''
