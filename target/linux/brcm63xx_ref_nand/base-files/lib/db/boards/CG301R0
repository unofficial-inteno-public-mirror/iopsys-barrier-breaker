	option lan_ok ''
	option lan_off ''
	option lan_notice ''
	option lan_alert ''
	option lan_error ''
	option status_ok     'Status_green=OFF Status_white=ON Status_red=OFF'
	option status_off    'Status_green=OFF Status_white=OFF Status_red=OFF'
	option status_notice 'Status_green=OFF Status_white=BLINK_SLOW Status_red=OFF'
	option status_alert  'Status_green=OFF Status_white=OFF Status_red=BLINK_SLOW'
	option status_error  'Status_green=OFF Status_white=OFF Status_red=BLINK_FAST'
